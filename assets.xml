<section name="assets">
	<pages>
		<page name="ng-head.min.js" test-coverage="false">
			<modules>
				<module name="site" controller="system" action="jspackage">
					<parameters>
						<script file="bower_components/jquery/jquery.min.js"/>
						<script file="node_modules/string-utils-cwm/lib/string-utils.min.js"/>
						<script file="bower_components/angular/angular.min.js"/>
					</parameters>
				</module>
			</modules>
		</page>
		<page name="ng-app.js" test-coverage="true">
			<modules>
				<module name="site" controller="system" action="jspackage">
					<parameters>
						<script file="public/js/app.js"/>
					</parameters>
				</module>
			</modules>
		</page>
		<page name="ng-style.css" test-coverage="false">
			<modules>
				<module name="site" controller="system" action="csspackage">
					<parameters>
						<link file="bower_components/bootstrap/dist/css/bootstrap.css"/>
						<link file="bower_components/bootstrap/dist/css/bootstrap-theme.css"/>
						<link file="public/css/app.css"/>
					</parameters>
				</module>
			</modules>
		</page>
	</pages>
</section>
